{
    "manifest_version": 3,
    "name": "Context Menu Extension",
    "version": "1.0",
    "permissions": ["contextMenus", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "icons": {
      "48": "icon.png"
    },
    "options_page": "options.html"
  }
  